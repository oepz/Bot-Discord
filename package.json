  "license": "MIT",
  "dependencies": {
    "discord.js": "discordjs/discord.js"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-aqua": "^7.1.2"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {

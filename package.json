{
  "name": "botdiscord",
  "version": "1.0.0",
  "description": "Discord bot by Juubi",
  "main": "main.js",
  "repository": "https://github.com/oepz/Bot-Discord.git",
  "author": "oepz <48292442+oepz@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "discordjs/discord.js",
    "mongo": "^0.1.0",
    "mongodb": "^3.3.4",
    "mongoose": "^5.7.11",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-aqua": "^7.1.2"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off",
      "no-trailing-spaces": "off",
      "eol-last": "off",
      "padded-blocks": "off",
      "function-paren-newline": "off",
      "prefer-template": "off",
      "no-eval": "off"
    }
  }
}
